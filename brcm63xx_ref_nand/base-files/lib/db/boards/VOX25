
config board 'board'
	option hardware 'VOX25'
	option hasAdsl '1'
	option hasVdsl '1'
	option hasVoice '1'
	option hasDect '0'
	option VoicePorts '2'
	option VoiceLeds '1'
	option hasWifi '1'
	option ethernetPorts '5'
	option ethernetPortNames 'LAN1 LAN2 LAN3 LAN4 WAN'
	option ethernetPortOrder 'eth0 eth1 eth2 eth3 eth4'
	option ethernetLanPorts 'eth0 eth1 eth2 eth3'
	option ethernetWanPort 'eth4'
	option adslWanPort 'atm0'
	option vdslWanPort 'ptm0'
	option lednames 'MMB WiFi VoIP DSL PowerLED'
	option buttonnames 'RESET WPS TOUCH_NEAR TOUCH_FAR'
	option hasBrcmAvs '1'
	option i2cGpioCustom 'bus0=0,14,9'

config wifi-chip '435f'
	option bands 'b'

config wifi-chip '4360'
	option bands 'a'

config buttons 'buttons'
	option RESET 'gpio 35 al resetbutton'
	option WPS 'gpio 34 al wpsbutton'
	option TOUCH_NEAR 'i2c 0 ah touch_near'
	option TOUCH_FAR 'i2c 1 ah touch_far'

config leds 'leds'
	option MMB_red 'spi 1 ah'
	option MMB_green 'spi 2 ah'
	option MMB_blue 'spi 3 ah'
	option WiFi_green 'spi 7 ah'
	option VoIP_red 'spi 5 ah'
	option VoIP_green 'spi 6 ah'
	option DSL_red 'spi 4 ah'
	option DSL_green 'spi 9 ah'
	option PowerLED_red 'spi 8 ah'

config led_map 'led_map'
	option dsl_ok 'DSL_green=ON DSL_red=OFF'
	option dsl_off 'DSL_green=OFF DSL_red=OFF'
	option dsl_notice 'DSL_green=BLINK_SLOW DSL_red=OFF'
	option dsl_alert 'DSL_green=OFF DSL_red=BLINK_SLOW'
	option dsl_error 'DSL_green=OFF DSL_red=ON'
	option wifi_ok 'WiFi_green=ON'
	option wifi_off 'WiFi_green=OFF'
	option wifi_notice 'WiFi_green=BLINK_SLOW'
	option wifi_alert 'WiFi_green=BLINK_SLOW'
	option wifi_error 'WiFi_green=BLINK_FAST'
	option wps_ok ''
	option wps_off ''
	option wps_notice ''
	option wps_alert ''
	option wps_error ''
	option lan_ok ''
	option lan_off ''
	option lan_notice ''
	option lan_alert ''
	option lan_error ''
	option status_ok 'PowerLED_red=ON'
	option status_off 'PowerLED_red=OFF'
	option status_notice 'PowerLED_red=BLINK_SLOW'
	option status_alert 'PowerLED_red=BLINK_SLOW'
	option status_error 'PowerLED_red=BLINK_FAST'
	option dect_ok ''
	option dect_off ''
	option dect_notice ''
	option dect_alert ''
	option dect_error ''
	option tv_ok ''
	option tv_off ''
	option tv_notice ''
	option tv_alert ''
	option tv_error ''
	option usb_ok ''
	option usb_off ''
	option usb_notice ''
	option usb_alert ''
	option usb_error ''
	option wan_ok ''
	option wan_off ''
	option wan_notice ''
	option wan_alert ''
	option wan_error ''
	option internet_ok 'MMB_green=ON'
	option internet_off 'MMB_green=OFF'
	option internet_notice 'MMB_green=BLINK_SLOW'
	option internet_alert 'MMB_green=BLINK_SLOW'
	option internet_error 'MMB_green=BLINK_FAST'
	option voice1_ok 'VoIP_green=ON VoIP_red=OFF'
	option voice1_off 'VoIP_green=OFF VoIP_red=OFF'
	option voice1_notice 'VoIP_green=BLINK_SLOW VoIP_red=OFF'
	option voice1_alert 'VoIP_green=OFF VoIP_red=BLINK_SLOW'
	option voice1_error 'VoIP_green=OFF VoIP_red=ON'
	option voice2_ok ''
	option voice2_off ''
	option voice2_notice ''
	option voice2_alert ''
	option voice2_error ''
	option eco_ok ''
	option eco_off ''
	option eco_notice ''
