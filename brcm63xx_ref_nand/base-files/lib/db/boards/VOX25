
config board 'board'
	option hardware 'V0X25'
	option hasAdsl '1'
	option hasVdsl '1'
	option hasVoice '1'
	option hasDect '0'
	option VoicePorts '2'
	option VoiceLeds '1'
	option hasWifi '1'
	option ethernetPorts '5'
	option ethernetPortNames 'LAN1 LAN2 LAN3 LAN4 WAN'
	option ethernetPortOrder 'eth0 eth1 eth2 eth3 eth4'
	option ethernetLanPorts 'eth0 eth1 eth2 eth3'
	option ethernetWanPort 'eth4'
	option adslWanPort 'atm0'
	option vdslWanPort 'ptm0'
	option lednames 'Status'
	option buttonnames 'RESET WPS'
	option hasBrcmAvs '1'
	option i2cGpioCustom 'bus0=0,14,9'

config wifi-chip '435f'
	option bands 'b'

config wifi-chip '4360'
	option bands 'a'

config buttons 'buttons'
	option RESET 'gpio 35 al resetbutton'
	option WPS 'gpio 34 al wpsbutton'

config leds 'leds'
	option Status_red 'gpio 17 ah'

